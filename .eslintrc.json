{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "jest": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "linebreak-style": "off",
        "no-unused-vars":"warn",
        "indent": "off",
        "no-console": "error",
        "no-trailing-spaces": "off",
        "comma-dangle": "off",
        "arrow-parens": "off",
        "prefer-destructuring": ["error", {
            "array": false,
            "object": true
        }]
    }
}
